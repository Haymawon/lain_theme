    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#C0C0C0",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "constant",
                "support.constant"
            ],
            "settings": {
                "foreground": "#FF55FF"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#55FF55"
            }
        },
        {
            "scope": [
                "keyword",
                "storage",
                "storage.type"
            ],
            "settings": {
                "foreground": "#FF5555",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted",
                "string.regexp"
            ],
            "settings": {
                "foreground": "#55FF55"
            }
        },
        {
            "scope": [
                "variable",
                "variable.parameter",
                "variable.other"
            ],
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#FF55FF",
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language"
            ],
            "settings": {
                "foreground": "#FF5555"
            }
        },
        {
            "scope": [
                "punctuation",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#FF5555",
                "background": "#000000"
            }
        },
        {
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#FF5555",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "markup.underline"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "markup.quote"
            ],
            "settings": {
                "foreground": "#C0C0C0",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.list"
            ],
            "settings": {
                "foreground": "#FF55FF"
            }
        },
        {
            "scope": [
                "markup.raw"
            ],
            "settings": {
                "foreground": "#55FF55"
            }
        },
        {
            "scope": [
                "meta.separator"
            ],
            "settings": {
                "foreground": "#FFFF00",
                "fontStyle": "bold"
            }
        }
    ]
}