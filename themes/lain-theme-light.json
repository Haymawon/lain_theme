    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#808080",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "constant",
                "support.constant"
            ],
            "settings": {
                "foreground": "#5E4B73"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#2C6B2C"
            }
        },
        {
            "scope": [
                "keyword",
                "storage",
                "storage.type"
            ],
            "settings": {
                "foreground": "#8B2C2C",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted",
                "string.regexp"
            ],
            "settings": {
                "foreground": "#2C6B2C"
            }
        },
        {
            "scope": [
                "variable",
                "variable.parameter",
                "variable.other"
            ],
            "settings": {
                "foreground": "#1A1A2E"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#5E4B73",
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language"
            ],
            "settings": {
                "foreground": "#8B2C2C"
            }
        },
        {
            "scope": [
                "punctuation",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#1A1A2E"
            }
        },
        {
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#8B2C2C",
                "background": "#FAF0FA"
            }
        },
        {
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#8B2C2C",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "markup.underline"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "markup.quote"
            ],
            "settings": {
                "foreground": "#808080",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.list"
            ],
            "settings": {
                "foreground": "#5E4B73"
            }
        },
        {
            "scope": [
                "markup.raw"
            ],
            "settings": {
                "foreground": "#2C6B2C"
            }
        },
        {
            "scope": [
                "meta.separator"
            ],
            "settings": {
                "foreground": "#C0A0C0",
                "fontStyle": "bold"
            }
        }
    ]
}